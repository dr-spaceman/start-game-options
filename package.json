{
    "name": "vgsite",
    "description": "A videogame site",
    "repository": {
        "type": "git",
        "url": "https://github.com/dr-spaceman/videogam.in"
    },
    "scripts": {
        "#build": "Generate bundles for production.",
        "build": "webpack --mode production --config webpack.production.js",
        "#watch": "Compile, and recompile on any changes.",
        "watch": "webpack --watch --config webpack.development.js",
        "#lint": "Runs ESLint on all relevant files",
        "lint": "eslint browser/src --ext js,jsx --ignore-pattern public/javascript",
        "#compile-apidoc": "Recompile API documentation based on redoc comments",
        "compile-apidoc": "npx redoc-cli bundle ./public/api/openapi.yaml --output ./public/api/index.html"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "classnames": "^2.2.6",
        "nodemon": "^2.0.4",
        "normalize.css": "^8.0.1",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-icons": "^3.11.0",
        "react-transition-group": "^4.4.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.10.4",
        "@babel/plugin-proposal-optional-chaining": "^7.12.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^4.3.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.9.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.2",
        "file-loader": "^6.1.0",
        "mini-css-extract-plugin": "^1.2.0",
        "redoc-cli": "^0.9.8",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.11",
        "sass-loader": "^10.0.2",
        "style-loader": "^1.2.1",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^5.1.4"
    }
}
